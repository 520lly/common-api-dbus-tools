		tRemoveConfigurationItem_itemElem2Dict elem2
	}

	map tRemoveConfigurationItem_optionsDict {
		String to Variant
	}

	map tUpdateConfigurationItem_old_itemElem2Dict {
		String to Variant
	}

	<**
