	attribute Boolean Ejectable

	attribute String SortKey

	attribute Boolean CanPowerOff

	attribute String SiblingId

	union Variant {
		String str
		Int16 i16
		Int32 i32
		UInt32 u32
		UInt16 u16
		Boolean b 
	}

}
