package commonapi.tests

interface TestPolymorphicInterface {
    version { major 1 minor 0 }

    method testPolymorphicStruct {
        in {
            TestBaseStruct testBaseStructIn
        }
        out {
            TestBaseStruct testBaseStructOut
        }
    }

    <** @experimental: polymorphic **>
    struct TestBaseStruct {
        UInt16 testBaseStructUInt16Value
        String testBaseStructStringValue
    }

    struct TestStructDerived1 extends TestBaseStruct {
        Int16 testStructDerived1Int16Value
    }

	struct TestStructDerived2 extends TestBaseStruct {
		Int32 testStructDerived2Int32Value
	}

    struct TestStructDerived2Derived extends TestStructDerived2 {
        UInt32 testStructDerived2DerivedUInt32Value
    }
}
